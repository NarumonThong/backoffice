                    v-model.trim=\"form.u_username\"\n                        :class=\"{ 'is-invalid': errors.has('u_username')}\"\n                        class=\"form-control\">\n                    <span class=\"invalid-feedback\">{{ errors.first('u_username') }}</span>\n                </div>\n\n                <div class=\"form-group\">\n                    <label for=\"\">รหัสผ่าน</label>\n                    <input type=\"password\" name=\"u_password\"\n                        v-validate=\"{ required: true, regex: /^[A-Za-z0-9]{4,15}$/ }\"\n                        v-model.trim=\"form.u_password\"\n                        :class=\"{ 'is-invalid': errors.has('u_password')}\"\n                        class=\"form-control\">\n                    <span class=\"invalid-feedback\">{{ errors.first('u_password') }}</span>\n                </div>\n\n                <div class=\"form-group\">\n                    <label for=\"\">ชื่อ</label>\n                    <input type=\"text\" name=\"u_firstname\"\n                        v-validate=\"'required'\"\n                        v-model.trim=\"form.u_firstname\"\n                        :class=\"{ 'is-invalid': errors.has('u_firstname')}\"\n                        class=\"form-control\">\n                    <span class=\"invalid-feedback\">{{ errors.first('u_firstname') }}</span>\n                </div>\n\n                <div class=\"form-group\">\n                    <label for=\"\">นามสกุล</label>\n                    <input type=\"text\" name=\"u_lastname\"\n                        v-validate=\"'required'\"\n                        v-model.trim=\"form.u_lastname\"\n                        :class=\"{ 'is-invalid': errors.has('u_lastname')}\"\n                        class=\"form-control\">\n                    <span class=\"invalid-feedback\">{{ errors.first('u_lastname') }}</span>\n                </div>\n\n                <div v-if=\"errorMessage\" class=\"alert alert-warning text-center\" >\n                    {{ errorMessage }}\n                </div>\n\n                <div class=\"form-group buttons\">\n                    <button type=\"submit\" class=\"btn btn-info btn-block\">\n                        ลงทะเบียน\n                    </button>\n\n                    <button type=\"button\" @click=\"onRedirectToLogin()\" class=\"btn btn-secondary btn-block\">\n                        เข้าสู่ระบบ\n                    </button>\n                </div>\n            </form>\n\n        </div>\n        </div>\n            \n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n    data() {\n        return {\n            form: {\n                u_username: \"\",\n                u_password: \"\",\n                u_firstname: \"\",\n                u_lastname: \"\",\n            },\n            errorMessage: \"\"\n        }\n    },\n    methods: {\n        // บันทึกข้อมูลลงทะเบียน\n        onsubmit() {\n            this.$validator.validateAll().then( valid => {\n                if (!valid) return;\n                axios\n                    .post('api/userlogin/register', this.form)\n                    .then(response => {\n                        this.onReset();\n                        this.onRedirectToLogin();\n                    })\n                    .catch(err => (this.errorMessage = err.response.data.message));\n            });\n        },\n        // ล้างค่า Form\n        onReset() {\n            this.errorMessage = null;\n            this.$validator.reset();\n            this.form = {\n                u_username: \"\",\n                u_password: \"\",\n                u_firstname: \"\",\n                u_lastname: \"\",\n            };\n        },\n        // ไปที่หน้าเข้าสู่ระบบ\n        onRedirectToLogin() {\n            this.$router.push({ name: \"login\" });\n        }\n    }\n};\n</script>\n\n<style scoped>\n.register {\n    max-width: 500px;\n    margin: 3% auto;\n}\n.img-logo {\n  width: 85%;\n  display: block;\n  margin: auto;\n  margin-bottom: 30px;\n}\nh1 {\n  font-size: 18px;\n  font-weight: 600;\n  text-align: center;\n  letter-spacing: 5px;\n}\n\n.card-body {\n  padding-left: 15%;\n  padding-right: 15%;\n}\n.buttons .btn-secondary {\n    margin-top: 20px;\n}\n.btn-info {\n    background-color: #D6DF23;\n}\n\n.buttons {\n  margin-top: 30px;\n  margin-bottom: 50px;\n}\n</style>\n\n\n"],"sourceRoot":"src/views"}]}